datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Todo {
  id    Int     @id @default(autoincrement())
  title String
  is_done  Boolean @default(false)
  created_at DateTime @default(now())
  updated_at DateTime @updatedAt
}

